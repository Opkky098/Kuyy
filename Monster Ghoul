local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded"))() --someone reuploaded it so I put it in place of the original back up so guy can get free credit.
local venyx = library.new("Rick Morty", 5013109572)

-- themes
local themes = {
	Background = Color3.fromRGB(255, 255, 255),
	Glow = Color3.fromRGB(255, 255, 255),
	Accent = Color3.fromRGB(255, 255, 255),
	LightContrast = Color3.fromRGB(255, 255, 255),
	DarkContrast = Color3.fromRGB(255, 255, 255),  
	TextColor = Color3.fromRGB(255, 255, 255)
}

local page = venyx:addPage("Main", 2062227389)
local jeez = venyx:addPage("Credit", 5012544693)
local section2 = jeez:addSection("Credit")
local section2 = jeez:addSection("Welcome "..game.Players.LocalPlayer.Name)
local section2 = jeez:addSection("Script By Realz Rick X#2333")
local section1 = page:addSection("Auto Farm")
section1:addDropdown("Pick Human and Cgg and Ghoul",{"Human","Low Level Aogiri","Rank 2 Investigator","High Level Aogiri","Mid Level Aogiri"}, function(t)
	_G.Select = t
end)
section1:addToggle("Start-Farm", nil, function(value)
	_G.AutoFarm1 = value
	local Stepped;
	Stepped = game:service'RunService'.RenderStepped:Connect(function()
		if _G.AutoFarm1 == true then
			pcall(function()
				game:service'Players'.LocalPlayer.Character.Humanoid:ChangeState(11)
			end)
		end
		if _G.AutoFarm1 == false then
			Stepped:Disconnect()
		end
	end)
	while _G.AutoFarm1 do wait()
		pcall(function()
			for i,v in pairs(game:GetService("Workspace").MidSpawns:GetChildren()) do
				if v.Name == _G.Select and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") then
					if v.Humanoid.Health > 0 then
						repeat  wait()   
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -5.5, 0)                                 
							if game:service'Players'.LocalPlayer:DistanceFromCharacter(v.HumanoidRootPart.Position) <= 20 then
								game:GetService("Players").LocalPlayer.Character.Remotes.KeyEvent:FireServer("Mouse1","Down")
							end
							v.Humanoid.WalkSpeed = 0
						until _G.AutoFarm1 == false or v.Humanoid.Health <= 0
					else
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 0, 0)
						if game.Players.LocalPlayer.Character.Humanoid.Health <= 0 then
							_G.AutoFarm = false
							wait(3)
							_G.AutoFarm = true

						end end end end end) end end)
section1:addToggle("Auto-Eat", nil, function(value)
	_G.Eat = value
	while _G.Eat do wait()
		pcall(function()
			for _,v in pairs(game:GetService("Workspace").MidSpawns:GetDescendants()) do
				if v.ClassName == "ClickDetector" and v.Parent ~= nil then
					fireclickdetector(v)
				end end end) end end)
section1:addToggle("Auto Kagune", nil, function(value)
	_G.AutoSpawnKagune = value
	while _G.AutoSpawnKagune do wait(.1)
		pcall(function()
			if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Kagune") == nil then
				game:GetService("Players").LocalPlayer.Character.Remotes.KeyEvent:FireServer("One","Down")
				wait(0.5)
			end end) end end)
section1:addToggle("Auto Mask", nil, function(value)
	_G.AutoSpawnKagune = value
	while _G.AutoSpawnKagune do wait(.1)
		pcall(function()
			if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Mask") == nil then
				game:GetService("Players").LocalPlayer.Character.Remotes.KeyEvent:FireServer("M","Down")
				wait(0.5)
			end end) end end)
section1:addToggle("Hide Character", nil, function(value)
	_G.Hide = value
	while _G.Hide do wait(.1)
		pcall(function()
			local PLAYER_NAME = "LocalPlayer"

			for i,v in pairs(game:service'Players'.LocalPlayer.Character.Head:GetDescendants()) do
				if v.ClassName == "BillboardGui" then
					v:remove()
				end
			end

			local http = game:GetService("HttpService")
			local insert = game:GetService("InsertService")
			local userId = game:GetService("Players"):GetUserIdFromNameAsync(PLAYER_NAME)
			local assets = http:JSONDecode(game:HttpGet('https://avatar.roblox.com/v1/users/' .. tostring(userId) .. '/currently-wearing'))
			local outfit = assets['assetIds']
			local plr = game:GetService("Players").LocalPlayer

			function destroyCurrent()
				for _, v in pairs(plr.Character:GetDescendants()) do
					if v:IsA("Pants") or v:IsA("Shirt") or v:IsA("Accessory") or v:IsA("Decal") or v:IsA("ShirtGraphic") then
						v:Destroy()
					end
				end
			end


			function createNew()
				for _, asset in pairs(outfit) do
					local asset = insert:LoadAsset(asset):GetChildren()[1]
					if asset:IsA("Decal") then
						asset.Parent = plr.Character.Head
					else
						asset.Parent = plr.Character
					end
				end
				if not plr.Character:FindFirstChildWhichIsA("Decal") then
					insert:LoadAsset(144075659):GetChildren()[1].Parent = plr.Character.Head
				end
			end


			if getgenv().connection then connection:Disconnect() end
			destroyCurrent()
			createNew()

			getgenv().connection = plr.CharacterAdded:Connect(function()
				plr.Character:WaitForChild("Shirt")
				destroyCurrent()
				createNew()    
			end)
		end)
	end
end)
local theme = venyx:addPage("Theme", 5012544693)
local colors = theme:addSection("Colors")

for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
	colors:addColorPicker(theme, color, function(color3)
		venyx:setTheme(theme, color3)
	end)
end

-- load
venyx:SelectPage(venyx.pages[1], true) -- no default for more freedom
while wait() do
	pcall(function()
		for i,v in ipairs(game:GetService("Workspace").MidSpawns:GetDescendants()) do
			if v:IsA("Model") then
				v.Parent = game:GetService("Workspace").MidSpawns
			end 
		end
		for i,v in ipairs(game:GetService("Workspace").LowLevelSpawns:GetDescendants()) do
			if v:IsA("Model") then
				v.Parent = game:GetService("Workspace").MidSpawns
			end 
		end
		for i,v in ipairs(game:GetService("Workspace").InvestigatorSpawns:GetDescendants()) do
			if v:IsA("Model") then
				v.Parent = game:GetService("Workspace").MidSpawns
			end 
		end
		for i,v in ipairs(game:GetService("Workspace").HighSpawns:GetDescendants()) do
			if v:IsA("Model") then
				v.Parent = game:GetService("Workspace").MidSpawns
			end 
		end
	end)
end
